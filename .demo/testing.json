{
  "$schema": "https://demotime.show/demo-time.schema.json",
  "title": "AAC API MileStone Demos",
  "version": 2,
  "demos": [
    {
      "id": "demo-mhs722sw-g3hz",
      "title": "switch branch to demo",
      "steps": [
        {
          "action": "openTerminal",
          "terminalId": "1"
        },
        {
          "action": "executeTerminalCommand",
          "command": "git switch demo-practice"
        }
      ]
    },
    {
      "id": "demo-mhpvgxug-h08g",
      "title": "npm install",
      "steps": [
        {
          "action": "openTerminal",
          "terminalId": "1"
        },
        {
          "action": "executeTerminalCommand",
          "command": "npm install aac-voice-api",
          "insertTypingMode": "character-by-character",
          "insertTypingSpeed": 50,
          "autoExecute": true,
          "terminalId": "1"
        }
      ]
    },
    {
      "id": "demo-mhpmjyyd-ob8v",
      "title": "Highlight empty code block",
      "steps": [
        {
          "action": "highlight",
          "path": "src/App.tsx",
          "startPlaceholder": "//d1s",
          "endPlaceholder": "//d1e"
        }
      ],
      "description": "highlights empty block of code to be filled in for initiate"
    },
    {
      "id": "demo-mhpo4tiw-cx5z",
      "title": "Add Offline",
      "steps": [
        {
          "action": "insert",
          "path": "src/App.tsx",
          "contentPath": "demofiles/offlineMode.js",
          "startPlaceholder": "//d2s",
          "insertTypingMode": "line-by-line",
          "insertTypingSpeed": 100
        },
        {
          "action": "highlight",
          "path": "src/App.tsx",
          "startPlaceholder": "//d2s",
          "endPlaceholder": "//d2e"
